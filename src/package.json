{
	"name": "attibling",
	"version": "1.1.1",
	"description": "Fusion between liebling and attila themes",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"get-database": "curl https://raw.githubusercontent.com/eddiesigner/ghost-db/ghost5.9.4/ghost.db -o ghost.db",
		"watch": "npx mix watch",
		"hot": "npx mix watch --hot",
		"docker-watch": "concurrently --names 'DOCKER,WEBPACK' --prefix-colors 'yellow,magenta' --kill-others \"docker-compose up\" \"npm run watch\"",
		"podman-watch": "podman-compose up -d && npm run watch",
		"deploy": "npm run production && node deploy/index.js",
		"production": "npm run build && npm run zip",
		"build": "npx mix --production",
		"zip": "run-script-os",
		"zip:win32": "cd .. && 7z a -r -x!.git -x!src -x!attibling.zip attibling.zip *",
		"zip:default": "cd .. && zip -r attibling.zip ./* -x '*.git*' -x '*src*' -x 'attibling.zip'",
		"order-locales": "node order-locales.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run order-locales; git add ../locales"
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"7zip": "0.0.6",
		"@babel/preset-react": "^7.24.7",
		"@tryghost/admin-api": "^1.13.12",
		"browser-sync": "^3.0.2",
		"browser-sync-webpack-plugin": "^2.3.0",
		"concurrently": "^8.2.2",
		"dotenv": "^16.4.5",
		"fs": "0.0.1-security",
		"ghost-cli": "^1.26.1",
		"husky": "^9.1.5",
		"laravel-mix": "^6.0.49",
		"path": "^0.12.7",
		"postcss": "^8.4.41",
		"resolve-url-loader": "5.0.0",
		"run-script-os": "^1.1.6",
		"sass": "^1.77.8",
		"sass-loader": "^16.0.1",
		"vue-template-compiler": "^2.7.16"
	},
	"dependencies": {
		"@tryghost/content-api": "^1.11.21",
		"fitvids": "^2.1.1",
		"fuse.js": "^7.0.0",
		"headroom.js": "^0.12.0",
		"jquery": "^3.7.1",
		"medium-zoom": "^1.1.0",
		"shave": "^5.0.4",
		"swiper": "^11.1.10",
		"tippy.js": "^6.3.7",
		"tocbot": "^4.29.0"
	},
	"optionalDependencies": {
		"win-node-env": "^0.6.1"
	}
}
